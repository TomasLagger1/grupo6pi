<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->
<%- include('./partials/head') %>
<body>
	<!-- Header -->
	<%- include('./partials/header') %>
	<!-- /Header -->
	
	<!-- Banner -->
	<!-- <div class="banner">
		<img src="images/img-home-banner.jpg" alt="Banner de ofertas">
	</div> -->
	<!-- /Banner -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Productos</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<% for (let i = 0; i < proddd.length; i++) { %> 
				<div id="seeyuh" class="col-12 col-sm-6 col-lg-3"> 
					<section class="product-box"> 
						<a href="/product"> 
							<figure class="product-box_image"> 
								<img src="<%= proddd[i].imagen %>" alt=""> 
							</figure> 
							<article class="product-box_data"> 
								<h2><%= proddd[i].producto %></h2> 
								<p><%= proddd[i].descripcion %></p> 
								<p>Comentarios: 12</p> 
							</article>
						</a> 
					</section> 
				</div> 
			<% } %> 
		</div>
	</div>


	<!-- footer -->
	<%- include('./partials/footer') %>
	<!-- /footer -->
</body>	
</html>